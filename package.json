{
	"name": "titanium-module-template",
	"version": "1.0.0",
	"description": "Native modules that allows you to to do something with Axway Titanium.",
	"main": "index.js",
	"scripts": {
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "tiapp-install-module",
		"postuninstall": "tiapp-uninstall-module",
		"prep": "npm install --ignore-scripts && npm run list",
		"prepublishOnly": "git pull && bump",
		"test": "echo \"no test specified\" && exit 0"
	},
	"author": "Axway",
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-module-template"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-module-template/issues"
	},
	"homepage": "https://github.com/brentonhouse/titanium-module-template#readme",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"titanium-native-module",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.something.ios",
				"name": "titanium-something-ios",
				"version": "1.2.3",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "9.0.0",
				"guid": ""
			},
			"android": {
				"moduleid": "ti.something.android",
				"name": "titanium-something-android",
				"version": "3.2.1",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86"
				],
				"minsdk": "9.0.0",
				"guid": ""
			}
		}
	},
	"devDependencies": {
		"@titanium/eslint-config": "0.0.12",
		"eslint": "^7.2.0",
		"fs-extra": "^9.0.1",
		"lodash": "^4.17.15"
	},
	"dependencies": {
		"@titanium/tiapp-xml": "^0.9.5"
	}
}
